{
	"_version": "1.1.0",
	"sap.app": {
		"_version": "1.1.0",
		"id": "zv.application.process",
		"type": "application",
		"i18n": "i18n/i18n.properties",
		"applicationVersion": {
			"version": "1.0.0"
		},
		"title": "{{appTitle}}",
		"description": "{{appDescription}}",
		"ach": "ach",
		"resources": "resources.json",
		"sourceTemplate": {
			"id": "servicecatalog.connectivityComponent",
			"version": "0.0.0"
		},
		"dataSources": {
			"GW_PROCESS_01_SRV": {
				"uri": "/sap/opu/odata/zco/GW_PROCESS_01_SRV/",
				"type": "OData",
				"settings": {
					"odataVersion": "2.0",
					"localUri": "webapp/localService/metadata.xml"
				}
			}
		}
	},
	"sap.ui": {
		"_version": "1.1.0",
		"technology": "UI5",
		"icons": {
			"icon": "",
			"favIcon": "",
			"phone": "",
			"phone@2": "",
			"tablet": "",
			"tablet@2": ""
		},
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		},
		"supportedThemes": [
			"sap_hcb",
			"sap_bluecrystal"
		]
	},
	"sap.ui5": {
		"_version": "1.1.0",
		"rootView": {
			"viewName": "zv.application.process.view.App",
			"type": "XML"
		},
		"dependencies": {
			"minUI5Version": "1.30.0",
			"libs": {
				"sap.ui.core": {},
				"sap.m": {},
				"sap.ui.layout": {}
			}
		},
		"contentDensities": {
			"compact": true,
			"cozy": true
		},
		"models": {
			"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"settings": {
					"bundleName": "zv.application.process.i18n.i18n"
				}
			},
			"oData": {
				"dataSource": "GW_PROCESS_01_SRV"
			}
		},
		"routing": {
			"config": {
				"routerClass": "sap.m.routing.Router",
				"viewType": "XML",
				"viewPath": "zv.application.process.view",
				"controlId": "idApp",
				"controlAggregation": "pages"
			},
			"routes": [
				{
					"pattern": "",
					"name": "processlist",
					"target": "processlist"
				},
				{
					"pattern": "process/{objid}",
					"name": "processdetails",
					"target": "processdetails"
				}
			],
			"targets": {
				"processlist": {
					"viewName": "ProcessList"
				},
				"processdetails": {
					"viewName": "ProcessDetails"
				}
			}
		}
	}
}